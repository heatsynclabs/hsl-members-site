    "@popperjs/core": "^2.11.8",
    "@react-jvectormap/core": "^1.0.4",
    "@react-jvectormap/world": "^1.1.2",
    "@react-router/node": "^7.9.2",
    "@react-router/serve": "^7.9.2",
    "apexcharts": "^5.3.5",
    "clsx": "^2.1.1",
    "flatpickr": "^4.6.13",
    "isbot": "^5.1.31",
    "motion": "^12.23.11",
    "prismjs": "^1.30.0",
