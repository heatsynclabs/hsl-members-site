{
  "name": "hsl-members-site",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "pnpm dlx serve -s build/client -p 3000",
    "typecheck": "react-router typegen && tsc",
    "lint": "prettier --check . && eslint .",
    "lint:fix": "prettier --write . && eslint --fix .",
    "typecheck:client": "react-router typegen && tsc --noEmit --skipLibCheck --project tsconfig.json",
    "build:docker": "docker build . -t hsl-web",
    "start:docker": "docker run -p 3000:3000 hsl-web"
  },
